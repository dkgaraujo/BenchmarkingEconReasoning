@article{gilboa2014analogies,
    author = {Gilboa, Itzhak and Postlewaite, Andrew and Samuelson, Larry and Schmeidler, David},
    title = "{Economic Models as Analogies}",
    journal = {The Economic Journal},
    volume = {124},
    number = {578},
    pages = {F513-F533},
    year = {2014},
    month = {07},
    abstract = "{People often wonder why economists analyse models whose assumptions are known to be false, while economists feel that they learn a lot from such exercises. We suggest that part of the knowledge generated by academic economists is case‚Äêbased rather than rule‚Äêbased. That is, instead of offering general rules or theories that should be contrasted with data, economists often analyse models that are ‚Äòtheoretical cases‚Äô, which help understand economic problems by drawing analogies between the model and the problem. Thus, economic models, empirical data, experimental results and other sources of knowledge are all on equal footing, that is, they all provide cases to which a given problem can be compared. We offer complexity arguments that explain why case‚Äêbased reasoning may sometimes be the method of choice and why economists prefer simple cases.}",
    issn = {0013-0133},
    doi = {10.1111/ecoj.12128},
    url = {https://doi.org/10.1111/ecoj.12128},
    eprint = {https://academic.oup.com/ej/article-pdf/124/578/F513/26375557/ecoj12128-sup-0001-appendix-a.pdf},
